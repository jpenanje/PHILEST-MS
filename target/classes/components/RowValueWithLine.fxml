<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<StackPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER">
         <children>
            <Text fx:id="value" strokeType="OUTSIDE" strokeWidth="0.0" text="20,000" />
            <HBox alignment="CENTER">
               <children>
                  <Text fx:id="percentage" fill="#a6a6a6" strokeType="OUTSIDE" strokeWidth="0.0" text="25%">
                     <font>
                        <Font size="9.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </Text>
                  <StackPane alignment="CENTER_LEFT">
                     <children>
                        <StackPane layoutX="10.0" layoutY="13.0" maxHeight="7.0" minHeight="7.0" prefHeight="7.0" prefWidth="65.0" style="-fx-background-color: F5F5F5; -fx-background-radius: 20;" />
                        <StackPane fx:id="progressPane" maxHeight="7.0" maxWidth="10.0" minHeight="7.0" minWidth="10.0" prefHeight="7.0" prefWidth="10.0" style="-fx-background-color: red; -fx-background-radius: 20;" />
                     </children>
                  </StackPane>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
