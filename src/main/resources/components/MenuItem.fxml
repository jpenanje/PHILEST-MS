<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>


<StackPane onMouseClicked="#onMouseClicked" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="image" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
   </children>
   <padding>
      <Insets bottom="16.0" left="20.0" right="16.0" top="16.0" />
   </padding>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</StackPane>
